{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@khulnasoft/static",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "rewrites": [
    {
      "source": "/new",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/old",
      "dest": "/index.html"
    }
  ]
}
